{
  "name": "mcp-giljabi",
  "version": "1.0.1",
  "description": "MCP 길잡이 - PlayMCP에서 적합한 MCP를 찾아주는 서비스",
  "main": "dist/index.js",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "node --experimental-fetch dist/httpServer.js",
    "start:stdio": "node --experimental-fetch dist/index.js",
    "dev": "node --experimental-fetch --import tsx src/httpServer.ts",
    "dev:stdio": "node --experimental-fetch --import tsx src/index.ts",
    "test-search": "node --experimental-fetch --import tsx src/scripts/testSearch.ts",
    "inspector": "npx @anthropic-ai/mcp-inspector node --experimental-fetch dist/index.js"
  },
  "keywords": [
    "mcp",
    "playmcp",
    "kakao",
    "ai"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "cors": "^2.8.5",
    "express": "^5.2.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^20.10.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  }
}
